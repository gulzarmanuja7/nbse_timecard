        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	if (current.isNewRecord())
		return;
	g_scratchpad.monthEndBetweenWeeks = false;
	var weekStart = new GlideDateTime(current.week_start.toString());
	var weekEnd = new GlideDateTime(current.week_end.toString());	
	if (weekStart.getMonthUTC() != weekEnd.getMonthUTC()) {
		g_scratchpad.monthEndBetweenWeeks = true;
	}

})(current, previous);]]></script>
